{
  "name": "cosmos-redux",
  "version": "1.2.3",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "frontlich",
  "module": "dist/cosmos-redux.esm.js",
  "size-limit": [
    {
      "path": "dist/cosmos-redux.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cosmos-redux.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "react-redux": "^7.2.0"
  },
  "repository": "https://github.com/frontlich/cosmos-redux.git",
  "keywords": [
    "redux",
    "redux-thunk",
    "redux-toolkit",
    "model"
  ],
  "license": "MIT"
}
